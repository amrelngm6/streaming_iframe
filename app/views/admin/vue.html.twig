<!DOCTYPE html>
<html dir="{{lang.translate('is_rtl')}}">

<head>


    <meta name="viewport"
        content="viewport-fit=cover, width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no" />
    <title>{{app.Settings.sitename}}</title>
    <link rel="stylesheet" type="text/css"
        href="https://fonts.googleapis.com/css?family=Material+Icons|Material+Icons+Outlined|Material+Icons+Two+Tone|Material+Icons+Round|Material+Icons+Sharp|Tajawal|Montserrat|Roboto:wght@300;400;500|Cairo:wght@300;500;700;900|Saira:wght@300;500;700;900">

    <script type="text/javascript">
        var enable_debug = {{ app.Settings.enable_debug ?1: '0' }};
        var root_url = '{{ app.CONF.url }}';
    </script>

    <link rel="stylesheet" crossorigin href="/src/assets/webfonts/fontawesome.min.css">
    <link rel="stylesheet" crossorigin href="/dist/assets/index.min.css">
    <script type="module" crossorigin src="/dist/assets/index.min.js"></script>
    
    <link rel="stylesheet" href="/src/front_assets/css/sweetalert2.min.css">

</head>

<body id="kt_app_body" data-kt-app-layout="dark-sidebar" data-kt-app-header-fixed="true"
    data-kt-app-sidebar-enabled="true" data-kt-app-sidebar-fixed="true" data-kt-app-sidebar-hoverable="true"
    data-kt-app-sidebar-push-header="true" data-kt-app-sidebar-push-toolbar="true"
    data-kt-app-sidebar-push-footer="true" data-kt-app-toolbar-enabled="true" class="app-default {{lang.translate('is_rtl')}}"
    data-kt-app-sidebar-minimize="off"
    style="direction: {{lang.translate('is_rtl')}};">




    <!--begin::App-->
    <div class="pt-10 px-4 w-full bg-gray-50" id="kt_app_root" >
        <!--begin::Page-->
        <div class="app-page  flex-column flex-column-fluid " id="kt_app_page">

            <!--begin::Wrapper-->
            <div class="app-wrapper  flex-column flex-row-fluid mt-10" id="kt_app_wrapper">

                <!--begin::Main-->
                <div class="app-main flex-column flex-row-fluid " id="kt_app_main">
                    <!--begin::Content wrapper-->
                    <div class="d-flex flex-column flex-column-fluid">

                        <!--begin::Content-->
                        <div id="kt_app_content" class="app-content  flex-column-fluid ">


                            <!--begin::Content container-->
                            <div id="kt_app_content_container" class="app-container  container-xxl ">

                                <!--end::Navbar-->
                                <!--begin::Row-->
                                <div class="row g-xxl-9">

                                    <div id="app" class="w-full p-0 block">
                                        {% include 'views/admin/nodata.html.twig' %}
                                    </div>

                                    <div id="root-parent" 
                                        data-page="{{app.request().requestUri|slice(1)}}"
                                        data-component="{{component}}" >
                                    </div>
                                </div>
                                <!--end::Row-->
                            </div>
                            <!--end::Content container-->
                        </div>
                        <!--end::Content-->

                    </div>
                    <!--end::Content wrapper-->


                    <!--begin::Footer-->
                    <div id="kt_app_footer" class="app-footer ">
                        <!--begin::Footer container-->
                        <div
                            class="app-container  container-fluid d-flex flex-column flex-md-row flex-center flex-md-stack py-3 ">
                            <!--begin::Copyright-->
                            <div class="text-gray-900 order-2 order-md-1">
                                <span class="text-muted fw-semibold me-1">2024&copy;</span>
                                <a href="https://medianssolutions.com" target="_blank"
                                    class="text-gray-800 text-hover-primary">Medians Solutions</a>
                            </div>
                            <!--end::Copyright-->

                        </div>
                        <!--end::Footer container-->
                    </div>
                    <!--end::Footer-->
                </div>
                <!--end:::Main-->


            </div>
            <!--end::Wrapper-->


        </div>
        <!--end::Page-->
    </div>
    <!--end::App-->

    <!--begin::Scrolltop-->
    <div id="kt_scrolltop" class="scrolltop" data-kt-scrolltop="true">
        <i class="ki-duotone ki-arrow-up"><span class="path1"></span><span class="path2"></span></i>
    </div>
    <!--end::Scrolltop-->

    <!--begin::Modals-->


    <style type="text/css">
        body .text-default {
            text-align: {{ app.lang=='english' ? 'left': 'right'}}
        }

        body,
        h1,
        h2,
        h3,
        h4,
        h5,
        h6 { 
            font-family: {{app.lang=='english' ? 'Saira': 'Cairo'}};
        }

        .active-booking-flash {
            -webkit-animation-name: move;
            animation-name: move;
            animation-duration: 10s;
            animation-delay: 1s;
            -webkit-animation-timing-function: linear;
            -webkit-animation-iteration-count: infinite;
            animation-iteration-count: infinite;
            -webkit-animation-fill-mode: both;
            animation-fill-mode: both;
        }

        @keyframes move {
            0% {
                opacity: 0;
            }

            5% {
                opacity: 1;
            }

            10% {
                opacity: 0;
            }

            15% {
                opacity: 1;
            }

            20% {
                opacity: 0;
            }

            25% {
                opacity: 1;
            }
        }
        body, html {
            font-family: {{ app.lang=='english' ? 'Saira': 'Tajawal'}}
        }
    </style>
    {#
    <script type="text/javascript" src="/dist/js/app.js"></script> #}


    <script src="https://html2canvas.hertzen.com/dist/html2canvas.min.js"></script>
</body>

</html>